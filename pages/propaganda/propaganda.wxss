.shareimg{
  width: 90%;
  height:auto;
  margin: auto
}
.shareimg image{
  width: 100%
}
.content{
  width: 90%;
  height: auto;
  background: #fff;
  font-size: 28rpx;
  box-sizing: border-box;
  padding:  30rpx ;
  margin: auto;
  border-radius: 10rpx;
  line-height: 50rpx;
  margin-top: 30rpx;
  box-shadow: 0rpx 0rpx 30rpx 5rpx rgb(247, 245, 245);
  margin-bottom: 30rpx
}
.share{
  width: 80%;
  height: 80rpx;
  background: #FC302F;
  text-align: center;
  line-height: 80rpx;
  color: #fff;
  font-size: 28rpx;
  margin: auto;
  border-radius: 45rpx;
  margin-bottom: 20rpx
}
.cover{
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0px;
  left: 0px;
  background: rgba(0, 0, 0, 0.6)
}
.shares{
  width: 100%;
  height: 300rpx;
  position: fixed;
  bottom: 0rpx;
  left: 0rpx;
  z-index: 100;
  background: #fff;
  text-align: center;
  box-sizing: border-box;
  padding-top: 30rpx
}
.shares .one{
  width: 150rpx !important;
  height: 200rpx !important;
  text-align: center;
  font-size: 24rpx;
  margin-top: 40rpx;
  padding: 0rpx;
  margin: 0rpx;
  list-style: none;
  background: #fff;
}
.shares .one image{
  width: 120rpx;
  display: block;
  margin: auto;
  margin-bottom: 10rpx
}
button::after{
border:none;
}

.win3{
  
  width: 100%;
  height: 100%;
  background: #EFEFEF;
  padding-top: 50rpx;
  position: fixed;

}
.youhui{
  display: block;
  width: 90%;
  margin: auto;
  margin-bottom: 40rpx
}
.youhuia{
  display: block;
  width: 80%;
  margin: auto;
  margin-bottom: 40rpx
}
.help{
  width: 70%;
  height: 80rpx;
  background: #FC302F;
  text-align: center;
  line-height: 80rpx;
  color: #fff;
  font-size: 28rpx;
  margin: auto;
  margin-bottom: 30rpx


}