.usertop{
  width: 100%;
  height: 240rpx;
  box-sizing: border-box;
  padding:0px 100rpx;
  line-height: 240rpx;
  font-size: 30rpx;
  /* border: 1px solid red */
}
.usertop image{
  width: 120rpx;
  height: 120rpx;
  border-radius: 50%;
  margin-top: 60rpx
}
.onebar{
  width: 100%;
  height: 120rpx;
  border-bottom: 4px solid #fdfbfb;
  box-sizing: border-box;
  padding: 0rpx 60rpx;
  line-height: 120rpx;
  font-size: 30rpx;

}
.onebar image{
  width: 30rpx;
  margin-top: 30rpx
}